{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "name": "Bookings",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "booked_from": {
      "type": "datetime"
    },
    "booked_to": {
      "type": "datetime"
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "amount": {
      "type": "float"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "PROGRESS",
        "COMPLETED",
        "SCHEDULED",
        "FAILED",
        "CANCELLED"
      ]
    },
    "booked_at": {
      "type": "datetime"
    },
    "user_id": {
      "type": "string"
    },
    "station_id": {
      "type": "string"
    },
    "payment_id": {
      "type": "richtext"
    },
    "payment_mode": {
      "type": "enumeration",
      "enum": [
        "ONLINE",
        "OFFLINE"
      ]
    },
    "payment_provider": {
      "type": "enumeration",
      "enum": [
        "RAZORPAY",
        "PAYUMONEY",
        "INSTAMOJO"
      ]
    },
    "connector_id": {
      "type": "string"
    },
    "mac_id": {
      "type": "string"
    }
  }
}
